<ul>
  <% @bookmarks.each do |bookmark| %>
    <li>
      <a href = "<%= bookmark.url %>">
      <%= bookmark.title %></a>
      </li>
  <% end %>
</ul>

<h1> Enter url </h1>
<form action = '/add' method ="post">
  <input type = "text" name="url"/>
  <input type = "text" name="title"/>
<input type = "submit" name="add_link" value="Submit"/>
</form>
